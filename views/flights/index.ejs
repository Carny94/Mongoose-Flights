
<h2>Flight List</h2>
<table>
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>Flight Number</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach((flight) => { %>
      <tr>
        <td><%= flight.airline %></td>
        <td><%= flight.airport %></td>
        <td><%= flight.flightNo %></td>
        <td><a href="/flights/<%= flight._id%>">INFO</a></td>
      </tr>
    <% }); %>
  </tbody>
</table>

